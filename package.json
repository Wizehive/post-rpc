{
  "name": "post-rpc",
  "version": "0.5.0",
  "description": "PostRPC - JSON RPC over PostMessage",
  "scripts": {
    "build": "webpack --mode=build",
    "deploy": "cp lib/PostRPC* example/post-rpc/app/js && cp lib/PostRPC* example/post-rpc/plugin/js",
    "dev": "webpack --progress --colors --watch --mode=dev",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "dependencies": {
    "native-promise-only": ""
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.22.1",
    "babel-cli":"^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.6.1",
    "mocha": "^3.2.0",
    "webpack": "^1.14.0",
    "yargs": "^4.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wizehive/post-rpc.git"
  },
  "keywords": [
  	"call",
    "json",
    "rpc",
    "postmessage",
    "iframe"
  ],
  "author": "David McNelis",
  "license": "MIT",
  "homepage": "https://github.com/wizehive/post-rpc"
}
